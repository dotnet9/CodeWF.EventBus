<Window x:Class="WPFDemo.MockManu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFDemo"
        mc:Ignorable="d"
        Title="手动注册处理" Height="400" Width="400">
    <StackPanel Margin="10">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="产品名称：" />
            <TextBox x:Name="TxtMessage" Width="200" />
            <Button Content="创建产品" Click="SendMessage_OnClick" Margin="10 0" />
            <Button Content="注销消息" x:Name="BtnEvent" Click="SubscribeOrUnsubscribe_OnClick" />
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="消息：" />
            <TextBox TextWrapping="Wrap" x:Name="TxtLog" Width="300" Height="300" />
        </StackPanel>
    </StackPanel>
</Window>